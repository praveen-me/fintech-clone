{
  "expo": {
    "name": "fintech_clone",
    "slug": "fintech_clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/base-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.praveen.fintech.clone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.praveen.fintech.clone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to upload for profile picture."
        }
      ],
      "react-native-compressor",
      [
        "expo-dynamic-app-icon",
        {
          "default": {
            "image": "./assets/images/base-icon.png",
            "prerendered": true
          },
          "blue": {
            "image": "./assets/images/light-blue.png",
            "prerendered": true
          },
          "light": {
            "image": "./assets/images/light-icon.png",
            "prerendered": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
